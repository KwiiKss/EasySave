        <ListView x:Name="listviewFR" Margin="63,161,45,53" MouseDown="LoadSavesFR"
>
        </ListView>
        <Label x:Name="Version" Content="Version 2.0" HorizontalAlignment="Left" Margin="554,418,0,0" VerticalAlignment="Top" Width="71" FontFamily="Segoe UI Black" FontWeight="Normal" FontSize="9" Height="22" Foreground="#FFFDFDFD"/>
        <Label x:Name="SourceText_Copy" Content="Voici le contenu de votre dossier de sauvegardes :" HorizontalAlignment="Center" Margin="0,121,0,0" VerticalAlignment="Top" Width="308" FontFamily="Segoe UI Black" FontWeight="Normal" Height="23" Foreground="#FFFDFDFD" RenderTransformOrigin="0.798,-2.322"/>
    </Grid>
</Window>